  <author email="rubanraj54@gmail.com">Rubanraj Ravichandran</author>
  <author email="bhargavi.mahesh@gmail.com">Bhargavi Mahesh</author>

  <maintainer email="vukcevicdjordjemne@gmail.com">Djordje Vukcevic</maintainer>
  <maintainer email="abepadalkar@gmail.com">Abhishek Padalkar</maintainer>
  <maintainer email="rubanraj54@gmail.com">Rubanraj Ravichandran</maintainer>
  <maintainer email="bhargavi.mahesh@gmail.com">Bhargavi Mahesh</maintainer>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>roscpp</build_depend>

  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>roscpp</run_depend>

</package>
